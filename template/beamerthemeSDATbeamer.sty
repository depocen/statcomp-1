\newcommand{\mysectionpage}{
	\begingroup
	\setbeamertemplate{headline}{}
	\setbeamertemplate{background}{
	  \begin{tikzpicture}
	  \useasboundingbox (0,0) rectangle(\the\paperwidth,\the\paperheight);
	  %\ifnum\thepage>1\relax% Not the title page	  
	  \draw[draw=col7,rounded corners=0.8ex,fill=col7] (-1,0) -- (-1,13) -- (13,13) -- (13,0) -- cycle;
	  \put(10,150){%
	  	\pgfuseimage{mybackground2}
	  }
	  \end{tikzpicture}
\addtocounter{framenumber}{-1}
}

	\setbeamercolor{section page}{fg=black}
	\setbeamertemplate{section page}{
		\vbox{}
		\vfill
		\begin{centering}

			\begin{beamercolorbox}[sep=8pt,center]{frametitle}
				\vskip-2cm \hskip-9cm
				Section \usebeamerfont{verse}\insertsectionnumber
				
				\vskip1.5cm
				\usebeamerfont{title2}\insertsection
			\end{beamercolorbox}
			
	
		\end{centering}
		\vfill
	}
	\frame{\sectionpage}
	\endgroup
}

